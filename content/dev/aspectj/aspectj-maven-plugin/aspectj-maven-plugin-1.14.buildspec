groupId=dev.aspectj
artifactId=aspectj-maven-plugin
display=${groupId}:${artifactId}
version=1.14

gitRepo=https://github.com/dev-aspectj/aspectj-maven-plugin.git
gitTag=${artifactId}-${version}

tool=mvn-3.9.6
jdk=11
