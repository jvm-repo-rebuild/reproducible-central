groupId=org.apache.jena
artifactId=jena
display=${groupId}:${artifactId}
version=5.2.0

gitRepo=https://github.com/apache/${artifactId}.git
gitTag=${artifactId}-${version}

tool=mvn-3.9.12
jdk=21
newline=lf
umask=022

command="mvn -Papache-release clean package -DskipTests -Dmaven.javadoc.skip -Dgpg.skip -Drat.skip -Dbuildinfo.ignore='*/jena-$version-cyclonedx.*'"
buildinfo=target/${artifactId}-${version}.buildinfo

#diffoscope=${artifactId}-${version}.diffoscope
issue=
PR_merge_max_ko=20
