groupId=org.spdx
artifactId=java-spdx-library
display=${groupId}:${artifactId}
version=1.1.9

gitRepo=https://github.com/spdx/Spdx-Java-Library.git
gitTag=v${version}

tool=mvn
jdk=17
newline=crlf
newlineGit=lf

command="mvn -Prelease clean verify -DskipTests -Dmaven.javadoc.skip -Dgpg.skip -Dspdx.skip -Dbuildinfo.ignore='*/*.spdx.json'"
buildinfo=target/${artifactId}-${version}.buildinfo

