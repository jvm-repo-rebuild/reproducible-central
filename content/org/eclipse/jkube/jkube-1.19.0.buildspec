groupId=org.eclipse.jkube
artifactId=jkube
version=1.19.0

display=${groupId}:${artifactId}

gitRepo=https://github.com/eclipse/${artifactId}.git
gitTag=v${version}

tool=mvn
jdk=8
newline=lf
umask=022
# require chmod g+s on directories for sources.jar
execBefore="find . -type d -exec chmod g+s {} \;"

# Rebuild command
command="mvn -Prelease clean package -DskipTests -Dmaven.javadoc.skip -Dgpg.skip -Dasciidoctor.skip"

buildinfo=target/${artifactId}-${version}.buildinfo
issue=
